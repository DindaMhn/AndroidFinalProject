<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu_provider_navigation"
    app:startDestination="@+id/homeProviderFragment">
    <action android:id="@+id/action_global_monthlyReportProviderfragment" app:destination="@id/monthlyReportProviderfragment"/>
    <action android:id="@+id/action_global_dailyReportProviderFragment" app:destination="@id/dailyReportProviderFragment"/>
    <fragment
        android:id="@+id/homeProviderFragment"
        android:name="com.example.androidfinalproject.screens.provider.HomeProviderFragment"
