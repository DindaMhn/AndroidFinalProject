<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu_provider_navigation"
    app:startDestination="@+id/homeProviderFragment">

    <fragment
        android:id="@+id/homeProviderFragment"
        android:name="com.example.androidfinalproject.screens.provider.HomeProviderFragment"
        android:label="fragment_home_provider"
        tools:layout="@layout/fragment_home_provider" >
        <action
            android:id="@+id/action_homeProviderFragment_to_addAssetFragment"
            app:destination="@id/addAssetFragment" />
    </fragment><action android:id="@+id/action_global_homeProviderFragment" app:destination="@id/homeProviderFragment"/>
    <fragment
